<div id="wrapper">
        <div class="row">
            <div class="col-md-6 col-md-offset-3 box-login">
                <div class="modal-content">
                    <div class="panel-heading">
                      <span><center><i class="fa fa-key fa-5x"></i></center></span>
                      </br>
                        <h1 class="page"><center>Inicia Sesión</center></h1>
                    </div>
                    <div class="panel-body">
                        <form role="form">
                            <fieldset>
<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs">
    <%= f.input :email,
                required: false,
                autofocus: true,
                input_html: { autocomplete: "email" } %>
    <%= f.input :password,
                required: false,
                input_html: { autocomplete: "current-password" } %>
    <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
  </div>

  <div class="form-actions">
    <center><%= f.button :submit, "Iniciar sesión", class:"btn btn-primary active" %></center>
  </div>
<% end %>
</br>
<%= render "devise/shared/links" %>
</fieldset>
</form>
</div>
</div>
</div>
</div>
</div>
