<div id="wrapper">
<div class="row">
<div class="col-lg-10">
<div class="col-lg-10">
<h1 class="page">REGISTRAR EGRESADO</h1>
</div>
<div class="panel panel-default">
<div class="panel-heading">
</br>
</br>
<div class="panel-body">
<%= simple_form_for @egresado do |f|%>


<div class="formulario">
  <div id="new">
  </br>

  <div class="form-group">
    <%= f.input :nombre,
     placeholder:"Digite Nombre"%>
  </div>

  <div class="form-group">
    <%= f.input :apellidos,
    placeholder:"Digite Apellidos"%>
  </div>

<div class="form-group">
  <%= f.input :ingreso%>
</div>

<div class="form-group">
  <%= f.input :egreso%>
</div>

<div class="form-group">
  <%= f.input :contingente, placeholder:"Digite Contingente"%>
</div>

<div class="form-group">
  <%= f.association  :facultad, label_method: :nombre%>
</div>

<div class="form-group">
  <%= f.association :programa, label_method: :nombre%>
</div>

<div class="form-group">
  <%= f.input :telefono,
   placeholder:"Digite Telefono"%>
</div>

<div class="form-group">
  <%= f.input :correo,
   placeholder:"Digite Correo"%>
</div>

<div class="form-group">
  <%= f.input :direccion,
   placeholder:"Digite Direccion"%>
</div>


<div class="actions">
  <center><%= f.submit 'Guardar', method: :index, data: { confirm: 'Guardar Egresado' },
  class:"btn btn-primary active"%></center>
</div>
</div>
<%end%>
</div>
 </div>
  </div>
  </div>
</div>
</div>
