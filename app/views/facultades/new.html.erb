<div class="container">
<div class="row">
<div class="col-lg-11">
<div class="col-lg-12">
<h1 class="page">REGISTRAR FACULTAD</h1>
</div>
<div class="panel panel-default">
<div class="panel-heading">
</br>
</br>
<div class="panel-body">
<%= simple_form_for @facultad do |f|%>


<div class="formulario">
  <div id="new">
  </br>

<div class="form-group">
  <%= f.input :nombre, placeholder: "Nombre Facultad"%>
</div>

<div class="form-group">
  <%= f.input :decano,
  placeholder:"Nombre Decano"%>
</div>

<div class="actions">
  <center>
  <%= f.submit 'Guardar', method: :new,
  data: { confirm: 'Guardar Facultad' },
   class:"btn btn-primary active"%>
</center>
</div>
</div>
<%end%>
</div>
</div>
</div>
</div>
</div>
</div>
